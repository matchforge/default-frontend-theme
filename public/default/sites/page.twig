{% extends "/default/base.html.twig" %}

{% block content %}
    {{ translateDictionary("page") }}


    <div class="content-wrap">
    <div class="mpl-navbar-mobile-overlay"></div>
    <div>
        {% if page.headerBg is defined %}
            <section class="mpl-banner mpl-banner-top mpl-banner-parallax mpl-banner-small">
                <div class="mpl-image" data-speed="0.8" style="z-index: 0;">
                    <div style="position: absolute; top: 0px; left: 0px; width: 100%; height: 100%; overflow: hidden; z-index: -100;" id="jarallax-container-0"><img src="{{ page.headerBg }}" alt="" class="jarallax-img" style="object-fit: cover; object-position: 50% 50%; max-width: none; position: absolute; top: 0px; left: 0px; width: 1902.8px; height: 502.52px; overflow: hidden; pointer-events: none; transform-style: preserve-3d; backface-visibility: hidden; will-change: transform, opacity; margin-top: 16.24px; transform: translate3d(0px, -16.26px, 0px);"></div></div>
                <div class="mpl-banner-content mpl-box-lg" style="filter: blur(0px);">
                    <div class="container">
                        <h1 class="display-1 mb-0">{{ page.title }}</h1>
                    </div>
                </div>
            </section>
        {% endif %}
        <div class="mpl-box-md">
            <div class="container">
                <h2>{{ page.title }}</h2>
                {{ page.content }}
            </div>
        </div>
    </div>
</div>


{% endblock %}